/dts-v1/;
/plugin/;

/ {
    fragment@0 {
        target = <&i2c>;
        __overlay__ {
            cst816s: cst816s@15 {
                compatible = "hynitron,cst816s";
                reg = <0x15>;
                interrupt-parent = <&gpio>;
                interrupts = <4 1>; /* IRQF_TRIGGER_RISING */
                reset-gpios = <&gpio 17 0>; /* GPIO_ACTIVE_HIGH */
                status="okay";
            };
        };
    };
};
